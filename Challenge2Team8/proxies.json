{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "getuser": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/api/proxy/getuser"
      },
      "backendUri": "https://serverlessohlondonuser.azurewebsites.net/api/GetUser"
    },
    "getproduct": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/api/proxy/getproduct"
      },
      "backendUri": "https://serverlessohlondonproduct.azurewebsites.net/api/GetProduct"
    },
    "getproducts": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/api/proxy/getproducts"
      },
      "backendUri": "https://serverlessohlondonproduct.azurewebsites.net/api/GetProducts"
    }
  }
}
